@model string
<td class="text-center">
    <a asp-action="Details" asp-route-id="@Model" class="btn btn-warning btn-sm">
        <i class="fas fa-info-circle"></i> Details
    </a>
</td>
<td class="text-center">
    <a asp-action="Edit" asp-route-id="@Model" class="btn btn-success btn-sm">
        <i class="fas fa-edit"></i> Update
    </a>
</td>
<td class="text-center">
    <a asp-action="Delete" asp-route-id="@Model" class="btn btn-danger btn-sm">
        <i class="fas fa-trash"></i> Delete
    </a>
</td>
